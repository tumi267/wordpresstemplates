{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/related-posts",
  "version": "0.1.0",
  "title": "Related Posts",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "attributes": {
    "postCount": {
      "type": "number",
      "default": 6
    },
    "fontFamily": {
      "type": "string",
      "default": "Inter, sans-serif"
    },
    "textColor": {
      "type": "string",
