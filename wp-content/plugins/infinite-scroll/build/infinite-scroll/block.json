{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/infinite-scroll",
  "version": "0.1.0",
  "title": "Infinite Scroll",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "attributes": {
    "postsPerPage": {
      "type": "number",
      "default": 10
    },
    "textColor": {
      "type": "string",
      "default": "#555555"
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "backgroundOpacity": {
      "type": "number",
      "default": 0
    },
    "cutoffdate": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "infinite-scroll",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}